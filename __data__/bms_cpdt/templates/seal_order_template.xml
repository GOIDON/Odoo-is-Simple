                        <div id="main-content"
                             class="col-md-3 document-template-categ"
                             style="margin-top: 0px; display: block; float: right;">


                            <div class="col-md-12"  style="height: auto; padding: unset;
                                margin: 15px 0 15px 0; float:right;">
                                <t t-set="event_data" t-value="website.get_last_7day_calendar_event()"/>
                                <div class="row" style="height: auto;
                                border: solid 1px #587E43; padding: 0; margin:0;">
                                    <h3 class="o_editable" style="text-align: center; font-family: inherit; color: #007dab;">
                                        <t t-esc="event_data['title']"/>
                                    </h3>
                                </div>
                            <t t-foreach="(1,2,3,4,5,6,7)" t-as="index">
                                <!--<div class="col-md-12">-->
                                <div class="row" style="height: auto; padding: 0; margin:0;">
                                    <div class="col-md-6 o_editable" style="height: auto;
                                    border: solid 1px #587E43; padding-left: -16px; padding-right: -16px;">
                                        <p style="text-align: center; color: red; padding-top: 10px;">
                                            <t t-if="event_data['week_day_dict'][index][1] != 8">
                                                <b>Thứ <t t-esc="event_data['week_day_dict'][index][1]"/></b>
                                            </t>
                                            <t t-if="event_data['week_day_dict'][index][1] == 8">
                                                <b>Chủ nhật</b>
                                            </t>
                                        </p>
                                    </div>
                                    <div class="col-md-6" style="height: auto;
                                    border: solid 1px #587E43; padding-left: -16px; padding-right: -16px;">
                                        <p style="text-align: center;  color: red;  padding-top: 10px;">
                                            <b class="o_editable"><t t-esc="event_data['week_day_dict'][index][0]"/></b>
                                        </p>
                                    </div>
                                </div>

                                <div class="row" style="height: auto;
                                border: solid 1px #587E43; padding: 0; margin: 0;">
                                    <t t-if="not event_data['event_dict'][event_data['week_day_dict'][index][0]]">
                                        <p style="text-align: left; padding: 10px 0 0 10px">
                                            Làm việc cả ngày
                                        </p>
                                    </t>
                                    <t t-foreach="event_data['event_dict'][event_data['week_day_dict'][index][0]]"
                                        t-as="event">
                                        <p style="text-align: left; padding: 10px 0 0 10px">
                                            <t t-esc="event.name"/>:
                                            <t t-esc="website.convert_utcdatetime_userdatetime(event.start)"/> đến
                                            <t t-esc="website.convert_utcdatetime_userdatetime(event.stop)"/>
                                        </p>
                                    </t>
                                    </div>
                                <!--</div>-->
                            </t>
                        </div>
                    </div>
                </div>
            </div>
        </t>
    </template>

